/* Exemplo de Programa na linguagem L -ERRO*/

integer n;
string nome;
byte a=-11,x=1;
boolean naoTerminou;
const MAXITER=10;

/* Programa Principal */
main

  write('Digite seu nome: ');
  readln(nome);
